package main;

public class main {

	public static void main(String[] args) {
		//imprimir los primeros diez numeros de la serie de fibonacci
		
		//0,1,1,2,3,5,8,13,21
		int n1=-1,n2=0,x;
		for (int i = 0; i < 10; i++) {
			
			
			x=n2;
			n2=n2+n1;
		    n1=x;
			System.out.println(n2);
			
			
		}

	}

}
