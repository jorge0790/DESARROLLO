package main;
    public class main {
     public static void main(String[] args) {
		// recorrer un arreglo con estructura for
		//declaramos el arreglo
		
		int numeros[]= {1,2,3,4,5,6,7,8,9};
		int suma=0;
		for (int i = 0; i < numeros.length; i++) {
	    suma=numeros[i]+suma;
	    
		}System.out.println(suma);
     }

}
