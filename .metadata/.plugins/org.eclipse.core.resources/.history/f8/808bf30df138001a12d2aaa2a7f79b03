package main;

public class main {

	public static void main(String[] args) {
		// estacion del año
		
		
		
	String mes="enero";
	switch(mes) {
	case "enero":
	case "febrero":
	case "marzo":
	case "abril":
		System.out.println("primer cuatrimestre");
		break;

	case "mayo":
	case "junio":
	case "julio":
	case "agosto":
		System.out.println("segundo cuatrimestre");
		break;
	case "septiembre":
	case "octubre":
	case "noviembre":
	case "diciembre":
		System.out.println("tercer cuatrimestre");
		break;
		
		default: System.out.println("opcion ingresada invalida");
	
	
	
	}
	
	
	
	
		
		
		
		
		
		
		
		
	}

}
