package main;

public class main {

	public static void main(String[] args) {
		// factorial de un numero
		//por ejemplo 4! es el resultado de multiplicar
		//1x2x3x4
		
		int numero=4;
		for (int i = 1; i <= numero; i++) {
			int factorial=i*(i+1);
			int almacenado=factorial;
			System.out.println(factorial);
			
		}

	}

}
