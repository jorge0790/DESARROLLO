package main;

public class main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		
		String color="verde";
		
		
		switch(color) {
		
		case "rojo":
			System.out.println("rojo");
		case "amarillo":
			System.out.println("amarillo");
		case "azul":
			System.out.println("azul");
			break;
			default:
				System.out.println("opcion no valida");
		
		
		
		
		}
		    
	}

}
