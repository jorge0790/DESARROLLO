package main;

public class main {

	public static void main(String[] args) {
		// el promedio minimo aprobatorio es 7
		//si el promedio es diez muchas felicidades 
		//si el promedio es menor a diez felicidades
		//si el promedio no es aprobatorio es necesario repasar bloques
		
		
	int promedio=10;
	
	
	
	if (promedio>=7) {
		
		        
	if(promedio==10) {System.out.println("muchas felicidades");
	else System.out.println("felicidades");
		
	}
		
		
	}else System.out.println("es necesario repasar el curso");
		
		
		
		
		

	}

}
