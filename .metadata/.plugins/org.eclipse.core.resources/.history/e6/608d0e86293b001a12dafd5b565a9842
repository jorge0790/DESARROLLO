package main;

public class main {

	public static void main(String[] args) {
	//aqui vemos que podemos declarar un arreglo en un
		//por convesion se colocan loss corchetes despues del nombre de la variable
		//podemos colocar los valores en la misma declaracion
		String  nombres[]=new String[3];
		nombres[0]="nombre1";
		nombres[1]="nombre1";
		nombres[2]="nombre1";
		
		
		

	}

}
