package main;

public class main {

	public static void main(String[] args) {
		//imprimir los primeros diez numeros de la serie de fibonacci
		
		//0,1,1,2,3,5,8,13,21
		int nBefore=0, nNext=1;
		for (int i = 0; i < 10; i++) {
			//nBefore nNext
			nNext=nBefore+nNext;
			System.out.println(nNext);
		}

	}

}
