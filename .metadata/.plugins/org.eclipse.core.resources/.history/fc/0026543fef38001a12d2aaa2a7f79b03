package main;

public class main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		
		
		
		int variable=2;
		
switch(variable){
case 1:
	System.out.println("el caso es 1");
	break;
case 2:
		System.out.println("el caso es 2");
case 3:
	System.out.println("el case es 3");
	default:  System.out.println("invalido");
	
	
}

	}

}
