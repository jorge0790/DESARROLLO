package main;

public class main {

	public static void main(String[] args) {
		//while estructura de control sobre iteracion, se sigue repitiendo hasta que una condicion deje de cumplirse
		//la expresion puede ser cualquiera que retorne un valor booleano
		//dentro de las llaves colocamos todo el codigio que queremos que se repita
		
		
		
		//while 
		int count=1;
		
		while(count <11) {
			System.out.println("hola mundo!");
			count+=1;
			
		}

	}

}
